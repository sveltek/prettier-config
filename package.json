{
  "name": "@sveltek/prettier-config",
  "version": "1.0.4",
  "author": "Sveltek",
  "contributors": [
    {
      "name": "Ivo Dolenc",
      "url": "https://github.com/ivodolenc"
    }
  ],
  "description": "Sveltek's config for Prettier.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveltek/prettier-config.git"
  },
  "homepage": "https://github.com/sveltek/prettier-config",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "sveltek",
    "svelte",
    "sveltekit",
    "prettier",
    "prettier-config",
    "presets",
    "config"
  ],
  "scripts": {
    "build": "hyperbundler",
    "format": "prettier --config src/index.js --write .",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "dependencies": {
    "prettier-plugin-svelte": "^3.4.1"
  },
  "devDependencies": {
    "@hypernym/bundler": "^0.31.0",
    "@sveltek/tsconfig": "^1.0.1",
    "prettier": "^3.7.4"
  }
}
